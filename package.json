{
    "name": "kobra-monorepo",
    "private": true,
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev --workspace=apps/web\" \"npm run dev --workspace=apps/server\"",
        "build": "npm run build --workspaces",
        "lint": "npm run lint --workspaces"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "dependencies": {
        "socket.io-client": "^4.8.3"
    }
}
